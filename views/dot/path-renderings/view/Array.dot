<ul>
  {{
    console.log(JSON.stringify(it.path));
    var psuedopath = {
      instance:it.path.caster.instance,
      options:it.path.caster.options
    };
  }}
  {{~ it.value :value:key}}
    <li>
      {{
        psuedopath.path = it.path.path+"."+key;
      }}
      {{=it.mongooseUI(psuedopath,value)}}
    </li>
  {{~}}
</ul>
