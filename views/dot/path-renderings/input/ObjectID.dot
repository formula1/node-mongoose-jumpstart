{{
var childmodel = it.utils.string2Model(it.path.options.ref);
if(typeof it.value == "undefined")
  it.value = "";
}}
<input type="text" name="{{=it.path.path}}" value="{{=it.value}}">
<script type="text/javascript">
  jQuery(function($){
    autocomplete(
      "[name={{=it.path.path}}]",
      "/{{=childmodel.modelName}}/search?_id={{=it.value._id}}",
      "_id"
    );
  });
</script>
